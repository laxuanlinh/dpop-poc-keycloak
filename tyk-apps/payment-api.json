{
  "name": "Payment API",
  "api_id": "payment-api",
  "use_openid": true,
  "openid_options": {
    "providers": [
      {
        "issuer": "http://keycloak:8080/realms/poc-linh",
        "client_ids": {
          "cG9jLWxpbmgtYXBw": "default"
        }
      }
    ]
  },
  "proxy": {
    "listen_path": "/payment/",
    "target_url": "http://host.docker.internal:8082",
    "strip_listen_path": true
  },
  "version_data": {
    "not_versioned": true,
    "versions": {
      "Default": {
        "name": "Default",
        "use_extended_paths": true
      }
    }
  },
  "active": true,
  "custom_middleware": {
    "driver": "grpc",
    "pre": [
      {
        "name": "DPoPCheck",
        "require_session": false
      }
    ]
  }
}
